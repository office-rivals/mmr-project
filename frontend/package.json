{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate-api": "openapi-generator-cli generate -i http://localhost:8081/swagger/v1/swagger.json -g typescript-fetch -o src/api --additional-properties=supportsES6=true --skip-validate-spec",
    "start": "node -r dotenv/config build"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.2.11",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte": "^5.0.0",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.5.0",
    "vite": "^5.4.4"
  },
  "type": "module",
  "dependencies": {
    "@carbon/charts-svelte": "^1.22.18",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.4",
    "bits-ui": "^1.0.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "d3-cloud": "^1.2.7",
    "d3-sankey": "^0.12.3",
    "dotenv": "^16.4.5",
    "formsnap": "^2.0.0",
    "lucide-svelte": "^0.474.0",
    "sveltekit-superforms": "^2.13.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.3.1",
    "zod": "^3.23.6"
  }
}
